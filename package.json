    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.2"
  },
  "scripts": {
    "start": "REACT_APP_LOCAL=true react-scripts start",
    "start:aws": "react-scripts start",
